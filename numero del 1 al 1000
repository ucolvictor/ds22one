 function Node(num) {
     this.num = num
     this.sig = null
  }
  
  
   let r=new Node(0)
   let p=new Node(1)
   r.sig=p
  	
  
  for (let i=0;i<100;i++){
    	let random = Math.floor(Math.random() * 1000 + 1);
      let n = new Node("name" + random)
      p.sig = n
      p=n
    	
  }
	
	console.log(r)
